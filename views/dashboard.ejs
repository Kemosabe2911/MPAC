<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>MPAC</title>
    <link rel="stylesheet" href="/css/style.css" type="text/css">
    <!--icons api-->
    <a href="https://icons8.com/icon/3260/sell"></a>
    <script src="https://kit.fontawesome.com/087042e108.js" crossorigin="anonymous"></script>
    <!--Jquery cdn-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
</head>
<body>
    <!--Navbar-->
    <nav>
        <ul>
            <li class="logo">MPAC</li>
            <li class="items"><a href="/home">Home</a></li>
            <li class="items"><a href="/dashboard">Dashboard</a></li>
            <li class="items"><a href="/about">About Us..</a></li>
            <li class="items"><a href="/contact">Contact Us</a></li>
            <li class="toggle"><span class="bars"></span></li>
        </ul>
    </nav>

    <!--User Info-->
    <section id="user-info">
        <div class="container">
            <div class="user-profile">
                <img src="https://img.icons8.com/office/80/000000/user.png"/>
                <!--<img src="https://img.icons8.com/ios-glyphs/100/000000/user-male-circle.png"/>-->
            </div>
            <div class="user-data">
                <div class="u-items">
                    <h2>Name</h2>
                    <h2>Email Id</h2>
                    <h2>Phone No</h2>
                </div>
                <div class="u-data">
                    <h2><%= name %></h2>
                    <h2><%= email %></h2>
                    <h2><%= phno %></h2>
                </div>
            </div>
        </div>
    </section>
    
    <!--User data -->
    <section id="tiles">
        <div class="container">
            <div id="my-prod" class="tile">
                <h2>My Products</h2>
                <img src="https://img.icons8.com/android/96/000000/buy.png"/>
                <!--<i class="fas fa-shopping-cart"></i>-->
            </div>
            <div id="my-cart" class="tile">
                <h2>My Cart</h2>
                <img src="https://img.icons8.com/android/96/000000/buy.png"/>
                <!--<i class="fas fa-clipboard-check"></i>-->
            </div>
        </div>
    </section>

    <script src="/js/main.js"></script>
    <script src='/js/jquery.js'></script>
</body>
</html>
