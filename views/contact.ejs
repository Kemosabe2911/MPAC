          <img src="/images/240_F_211309674_wutVgt4EpBSHhFCDskloGEnuzCQsz0Dl.jpg">
      </div>
      <div class="contact-form">
          <h1>Contact Us</h1>
          <span></span>
          <form action="/contact-mail" method="POST" >
              <label for="name">Name:</label>
              <input id="name" type="text" name="Name" class="input1" required maxlength="200" placeholder="Enter Name">
        
              <label for="email">Email Address:</label>
              <input id="email" type="email" name="Email" class="input1" required maxlength="200" placeholder="Enter Email-id">
        
              <label for="message">Message:</label>
              <textarea id="message" name="Message" rows="10" maxlength="6000" placeholder="Text Message" required></textarea>
          <div class="contact-btn-container">
              <input type="submit" class="contact-btn" value="Send Message">
          </div>
          </form>
      </div>
